    <Exec Command="lib %22%(ExtractItem.LibFile)%22 /EXTRACT:%(ExtractItem.Identity) /out:%22%(ExtractItem.OutDir)%(ExtractItem.Filename)%(ExtractItem.Extension)%22 /MACHINE:$(MACHINE)" />
    <ItemGroup>
      <Obj Include="%(ExtractItem.OutDir)%(ExtractItem.Filename)%(ExtractItem.Extension)">
        <LinkCompiled>true</LinkCompiled>
        <LibCompiled>true</LibCompiled>
        <ImpLibCompiled>true</ImpLibCompiled>
      </Obj>
    </ItemGroup>
  </Target>
  <!--生成只有ucrtbase导出的vcrt动态库-->
  <Target Name="Build_ucrtbase_lib" AfterTargets="Lib" Inputs="$(SolutionDir)bin\$(TargetVCRuntimeVersion)\$(Platform)\vcruntime.lib" Outputs="$(IntDir)ucrtbase.lib">
    <PropertyGroup>
      <vcruntime_path>$(SolutionDir)bin\$(TargetVCRuntimeVersion)\$(Platform)\vcruntime.lib</vcruntime_path>
      <vcruntime_path Condition="'$(Platform)' == 'ARM64'">$(IntDir)vcruntime_no_ec.lib</vcruntime_path>
    </PropertyGroup>
    <!--如果是arm64，那么删除ec相关符号，我们暂时不做支持-->
    <Copy SourceFiles="$(SolutionDir)bin\$(TargetVCRuntimeVersion)\$(Platform)\vcruntime.lib" DestinationFiles="$(IntDir)vcruntime_no_ec.lib" OverwriteReadOnlyFiles="true" Condition="'$(Platform)' == 'ARM64'" />
    <Exec Command="lib $(vcruntime_path) /remove:123" Condition="'$(Platform)' == 'ARM64'" />
    <Exec Command="%22$(SolutionDir)bin\LibMaker.exe%22 ExportDef %22$(vcruntime_path)%22 /MACHINE:$(MACHINE) /OUT:%22$(IntDir)ucrtbase.def%22" />
    <Exec Command="lib /def:%22$(IntDir)ucrtbase.def%22 /out:%22$(IntDir)ucrtbase.lib%22 /MACHINE:$(MACHINE)" />
    <!--复制lib属性，Win32下面 __seh_longjmp_unwind@4 这类接口需要标记导出类型-->
    <Exec Command="%22$(SolutionDir)bin\LibMaker.exe%22 CopyLibType %22$(IntDir)ucrtbase.lib%22 %22$(vcruntime_path)%22" />
  </Target>
  <Target Name="Build_vcruntime_lib" AfterTargets="Build_ucrtbase_lib" Condition="'$(Configuration)' == 'Dynamic'" Inputs="$(IntDir)ucrtbase.lib;$(OutDir)$(TargetName)$(TargetExt)" Outputs="$(OutDir)vcruntime.lib">
    <Copy SourceFiles="$(OutDir)$(TargetName)$(TargetExt)" DestinationFiles="$(OutDir)vcruntime.lib" OverwriteReadOnlyFiles="true" />
    <!--合并-->
    <Exec Command="lib %22$(OutDir)vcruntime.lib%22 %22$(IntDir)ucrtbase.lib%22" />
  </Target>
  <Target Name="Build_libvcruntime_lib" AfterTargets="Build_ucrtbase_lib" Condition="'$(Configuration)' == 'Static'" Inputs="$(IntDir)ucrtbase.lib;$(OutDir)$(TargetName)$(TargetExt)" Outputs="$(OutDir)libvcruntime.lib">
    <Copy SourceFiles="$(OutDir)$(TargetName)$(TargetExt)" DestinationFiles="$(OutDir)libvcruntime.lib" OverwriteReadOnlyFiles="true" />
    <!--合并-->
    <Exec Command="lib %22$(OutDir)libvcruntime.lib%22 %22$(IntDir)ucrtbase.lib%22" />
  </Target>
</Project>