      SourceFiles="$(UniversalCRTSdkDir)Lib\$(WindowsTargetPlatformVersion)\ucrt\arm\ucrt.lib"
      DestinationFiles="$(MSBuildThisFileDirectory)TargetPlatform\10.0.19041.0\lib\ARM\libucrt.lib"
      />
    
    <Copy
      SourceFiles="$(UniversalCRTSdkDir)Lib\$(WindowsTargetPlatformVersion)\ucrt\arm64\ucrt.lib"
